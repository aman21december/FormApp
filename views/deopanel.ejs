    <!-- <%=collegeData[0].addresses[0].Location%> -->
    <div style="text-align: right;">
        <form action="/v1/auth/logout">
            <input type="submit" value="Logout">
        </form>
    </div>
    
    <%collegeData.forEach(college=>{%>
        <h1><%=college.name%></college.name></h1>
        <% if(college.BasicInformations && college.BasicInformations.length > 0) { %>
            <h3>Basic Information</h3>
            <table border="1" cellspacing="5" cellpadding="0">
                <thead>
                    <tr>
                        <th>AISHECODE</th>
                        <th>Name Of Institution</th>
                        <th>Year Of Establishment</th>
                        <th>Name Of The University</th>
                        <th>Whether the institution is exclusively meant for students from one gender</th>
                        <th>Whether the institution has NAAC status</th>
                        <th>In case yes, please give grading details</th>
                    </tr>
                </thead>
                <tbody>
                    <% college.BasicInformations.forEach(BasicInformation => { %>
                        <tr>
                            <td><%= BasicInformation.AISHECODE %></td>
                            <td><%= BasicInformation.NameOfInstitution %></td>
                            <td><%= BasicInformation.YearOfEstablishment %></td>
                            <td><%= BasicInformation.NameOfTheUniversity %></td>
                            <td><%= BasicInformation.gender %></td>
                            <td><%= BasicInformation.NAACStatus %></td>
                            <% if (BasicInformation.GradingDetails) { %>
                                <td><%= BasicInformation.GradingDetails %></td>
                            <% } else { %>
                                <td>N/A</td>
                            <% } %>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        <% } %>
        
          <% if(college.OfficersDetails && college.OfficersDetails.length > 0) { %>
            <h3>Officers Details</h3>
            <table border="1" cellspacing="5" cellpadding="0">
                <thead>
                    <tr>
                        <th>Name of Vice Chancellor/Principal</th>
                        <th>In case of College Full Time Principal</th>
                        <th>College Email</th>
                        <th>Head of Institution Email Id</th>
                        <th>Mobile Number</th>
                        <th>Telephone Number (With STD Code)</th>
                    </tr>
                </thead>
                <tbody>
                    <% college.OfficersDetails.forEach(OfficersDetail => { %>
                        <tr>
                            <td><%= OfficersDetail.ViceChancellor %></td>
                            <td><%= OfficersDetail.FULLTIMEPRINCIPAL %></td>
                            <td><%= OfficersDetail.email %></td>
                            <td><%= OfficersDetail.CollegeHeadEmail %></td>
                            <td><%= OfficersDetail.Mobile %></td>
                            <td><%= OfficersDetail.Telephone %></td>
                        </tr>
                    <% }) %>
                </tbody>
            </table>
        <% } %>
        

